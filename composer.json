{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "bugsnag/bugsnag-laravel": "^2.0",
        "fideloper/proxy": "^4.0",
        "hipsterjazzbo/landlord": "dev-laravel-57",
        "insenseanalytics/laravel-nova-permission": "^0.1.2",
        "laravel/cashier": "~8.0",
        "laravel/framework": "5.7.*",
        "laravel/nova": "~1.0",
        "laravel/spark-aurelius": "@dev",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "laravelista/comments": "^2.1",
        "lavary/laravel-menu": "^1.7",
        "spatie/laravel-newsletter": "^4.2",
        "spatie/laravel-permission": "^2.31",
        "venturedrake/laravel-boilerplate": "dev-master",
        "yoeunes/notify": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Http/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type":"git",
            "url":"git@bitbucket.org:venturedrake/laravel-boilerplate.git"
        },
        {
          "type": "composer",
          "url": "https://nova.laravel.com"
        },
        {
          "type": "path",
          "url": "./spark"
        },
        {
          "type": "vcs",
          "url":  "git@github.com:hmazter/Landlord.git"
        }
    ]
}
